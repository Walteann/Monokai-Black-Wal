	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#546E7A"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder",
			],
			"settings": {
				"foreground": "#678CB1"
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#FF5370"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				// "keyword",
				"storage.type",
				"storage.modifier",
				"entity.name.function.dart"
			],
			"settings": {
				"foreground": "#66d9ef",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Variables Dart",
			"scope": [
				"variable.parameter.dart"
			],
			"settings": {
				"foreground": "#74E6DB",
			}
		},
		{
			"name": "Function dart",
			"scope": [
				"entity.name.function.dart"
			],
			"settings": {
				"foreground": "#2EA6E2",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				// "keyword.control",
				"constant.other.color",
				"punctuation",
				"meta.tag",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.section.embedded",
				"keyword.other.template",
				"keyword.other.substitution"
			],
			"settings": {
				"foreground": "#FFFFFF"
			}
		},
		{
			"name": "Keywords #f92672",
			"scope": [
				"keyword.control.import.ts",
				"keyword.control.export.ts",
				"keyword.other.import.dart",
				"keyword.declaration.dart",
				"storage.modifier.ts",
				"keyword.control.from",
				"keyword.control",
				"keyword.operator",
				"source.ini",
				"keyword.other.special-method.dockerfile"
				
			],
			"settings": {
				"foreground": "#f92672"
			}
		},
		{
			"name": "Keywords #f92672 and italic",
			"scope": [
				"keyword.control.from",
				
			],
			"settings": {
				"foreground": "#f92672",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Keywords reserved",
			"scope": [
				"variable.parameter",
				"entity.other.attribute-name.id",
